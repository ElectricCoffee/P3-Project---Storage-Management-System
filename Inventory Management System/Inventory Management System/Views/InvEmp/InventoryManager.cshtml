<div class="container-fluid">
    <div class="row">

        <div class="col-md-8"><!--add stuff here--></div>
    </div>
</div>

<table class="table table-bordered">
    <tr>
        <th>Article Number</th>
        <th>Name</th>
        <th>Serial Number</th>
        <th>Inventory Locatione</th>
        <th>World Locatione</th>
        <th>Transit</th>
        <th>Inventory Status</th>
        <th>Model</th>
        <th>Amount</th>
        <th>Production Year</th>
        <th>Category</th>
        <th>Tags</th>
        <th>Acquisitor</th>
        <th>A-price</th>
        <th>Sales Status</th>
        <th>Number of Files</th>
        <th>Spec Sheets</th>
    </tr>
    <tr ng-repeat="data in ac.tableData">
        <td>{{data.articleNumber}}</td>
        <td>{{data.name}}</td>
        <td>{{data.serialNumber}}</td>
        <td>{{data.inventoryLocation}}</td>
        <td>{{data.worldLocation}}</td>
        <td>{{dta.transit}}</td>
        <td>{{data.inventoryStatus}}</td>
        <td>{{data.model}}</td>
        <td>{{data.amount}}</td>
        <td>{{data.productionYear}}</td>
        <td>{{data.category}}</td>
        <td>{{data.tags}}</td>
        <td>{{data.acquisitor}}</td>
        <td>{{data.aPrice}}</td>
        <td>{{data.salesStatus}}</td>
        <td>{{data.numberOfFiles}}</td>
        <td>{{specSheets}}</td>
    </tr>
</table>


