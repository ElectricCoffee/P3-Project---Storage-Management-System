<div ng-controller="AccountantController as ac" 
     ng-init="ac.productService.getTableData()">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-8"><!--add stuff here--></div>
        </div>
    </div>
    <table class="table table-bordered">
        <tr>
            <th>Article Number</th>
            <th>Name</th>
            <th>Serial Number</th>
            <th>Sales Price</th>
            <th>Sales Status</th>
            <th>Amount</th>
            <th>Acquisitor</th>
            <th>Acquisition Price</th>
            <th>Category</th>
            <th>Model</th>
            <th>Tags</th>
        </tr>
        <tr ng-repeat="data in ac.productService.tableData">
            <td>{{data.ArticleNumber1}}</td>
            <td>{{data.Name}}</td>
            <td>{{data.SerialNumber}}</td>
            <td>{{data.SalesPrice}}</td>
            <td>{{data.SalesStatus}}</td>
            <td>{{data.Amount}}</td>
            <td>{{data.Acquisitor}}</td>
            <td>{{data.AcquisitionPrice}}</td>
            <td>{{data.Category}}</td>
            <td>{{data.Model}}</td>
            <td>{{data.Tags}}</td>
        </tr>
    </table>
    <div class="col-md-4">
        <a href="/Accountant/Edit"
           class="button btn btn-primary button-gui"
           target="_blank">Edit Product</a>
    </div>
</div>
